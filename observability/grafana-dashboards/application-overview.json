{
  "dashboard": {
    "title": "Agentic Workflow - Application Overview",
    "tags": ["agentic", "application", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{path}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_bucket[5m]))",
            "legendFormat": "{{path}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{category}} - {{severity}}"
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Requests",
        "type": "stat",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "http_requests_active"
          }
        ]
      },
      {
        "id": 5,
        "title": "Total Users",
        "type": "stat",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "users_total"
          }
        ]
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "process_memory_heap_used",
            "legendFormat": "Heap Used"
          },
          {
            "expr": "process_memory_heap_total",
            "legendFormat": "Heap Total"
          }
        ]
      },
      {
        "id": 7,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(process_cpu_user_seconds_total[5m])",
            "legendFormat": "User"
          },
          {
            "expr": "rate(process_cpu_system_seconds_total[5m])",
            "legendFormat": "System"
          }
        ]
      }
    ]
  }
}