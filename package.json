{
  "name": "agentic-workflow",
  "version": "1.0.0",
  "description": "Modern AI-powered development workflow optimized for Claude Code CLI",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && vite build",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "start:test": "NODE_ENV=test node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:smoke": "jest --testPathPattern=smoke",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:changed": "jest -o",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint **/*.{css,scss}",
    "lint:markdown": "markdownlint **/*.md",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "complexity": "plato -r -d reports/complexity src",
    "dead-code": "ts-prune",
    "docs:api": "typedoc --out docs/api src",
    "docs:code": "jsdoc -c jsdoc.config.json",
    "docs:build": "npm run docs:api && npm run docs:code",
    "docs:serve": "serve docs",
    "docs:coverage": "documentation coverage src/**/*.ts",
    "docs:update": "npm run docs:build",
    "analyze:bundle": "vite-bundle-visualizer",
    "benchmark": "node benchmarks/run.js",
    "profile": "0x -o dist/index.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:test": "NODE_ENV=test knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "security:check": "npm audit && snyk test",
    "performance:check": "lighthouse http://localhost:3000 --output json --output-path ./reports/lighthouse.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "prepush": "npm run test:unit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.40.0",
    "@opentelemetry/exporter-prometheus": "^0.46.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.46.0",
    "@opentelemetry/instrumentation-express": "^0.34.0",
    "@opentelemetry/instrumentation-http": "^0.46.0",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/sdk-metrics": "^1.19.0",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "knex": "^3.0.0",
    "pg": "^8.11.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.0",
    "redis": "^4.6.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@anthropic/claude-code-cli": "latest",
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@playwright/test": "^1.40.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "0x": "^5.7.0",
    "conventional-changelog-cli": "^4.1.0",
    "documentation": "^14.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.0",
    "k6": "^0.47.0",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "markdownlint-cli": "^0.37.0",
    "nodemon": "^3.0.0",
    "patch-package": "^8.0.0",
    "plato": "^1.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.1.0",
    "serve": "^14.2.0",
    "snyk": "^1.1266.0",
    "stylelint": "^16.0.0",
    "supertest": "^6.3.0",
    "trivy": "^0.0.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "ts-prune": "^0.10.0",
    "tsx": "^4.6.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-bundle-visualizer": "^0.11.0",
    "wait-on": "^7.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint --fix"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/tests"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/*.spec.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/verlyn13/agentic-workflow.git"
  },
  "keywords": [
    "ai",
    "claude",
    "workflow",
    "automation",
    "observability",
    "development"
  ],
  "author": "verlyn13",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/verlyn13/agentic-workflow/issues"
  },
  "homepage": "https://github.com/verlyn13/agentic-workflow#readme"
}